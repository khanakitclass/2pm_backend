{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/public/(.*)",  
            "dest": "/public/$1"
        },
        {
            "src": "/api.yaml",
            "dest": "/src/api.yaml"
        },
        {
            "src": "/(.*)", 
            "dest": "/src/index.js"
        }
    ]
}
